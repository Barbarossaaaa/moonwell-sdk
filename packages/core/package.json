{
  "name": "@moonwell-sdk/core",
  "version": "0.0.1",
  "description": "Moonwell SDK",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/moonwell-fi/moonwell-sdk",
    "directory": "packages/core"
  },
  "files": ["dist", "src/**/*.ts", "!src/**/*.test.ts", "!src/**/*.test-d.ts", "!src/_test/**/*"],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:typecheck": "vitest --typecheck.only",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "hono": ">=4.5",
    "typescript": ">=5.0.4",
    "viem": "^2.19.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "vitest": "^2.0.5"
  },
  "imports": {
    "@moonwell-sdk/environments": "../environments/index.ts"
  },
  "engines": {
    "node": ">=18.14"
  }
}
